    }
}

pipeline {
    agent any // This specifies that the pipeline can run on any available agent

    triggers {
        cron('H H * * 2') // Scheduled to run every Tuesday
    }

    stages {
        stage('Second Pipeline') {
            steps {
                echo 'Running second pipeline'
                // Add your steps for the second pipeline here
            }
        }
    }
}
